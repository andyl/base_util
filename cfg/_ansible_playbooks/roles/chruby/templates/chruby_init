export PATH="/home/{{rem_user}}/.chruby/shims:${PATH}"
source "/home/{{rem_user}}/.chruby/libexec/../completions/chruby.bash"
chruby rehash 2>/dev/null
chruby() {
  typeset command
  command="$1"
  if [ "$#" -gt 0 ]; then
    shift
  fi

  case "$command" in
  rehash|shell)
    eval "`chruby "sh-$command" "$@"`";;
  *)
    command chruby "$command" "$@";;
  esac
}
